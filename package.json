    "url": "git+https://github.com/eyeree/partoken.git"
  },
  "keywords": [
    "html",
    "canvas",
    "particle",
    "nft",
    "token",
    "interactive"
  ],
  "author": "eyeree",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eyeree/partoken/issues"
  },
  "homepage": "https://github.com/eyeree/partoken#readme",
  "dependencies": {
    "@types/chroma-js": "^2.1.3",
    "chroma-js": "^2.1.2",
    "html-inline": "^1.2.0",
    "typescript": "^4.5.4"
  }
}
